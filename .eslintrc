{
  "extends": "airbnb",
  "env": { "meteor": true },
  "settings": {
    "import/core-modules": ["meteor/meteor"]
  },
  "rules": {
    "class-methods-use-this": "off",
    "import/no-extraneous-dependencies": "off",
    "import/no-unresolved": "off",
    "import/extensions": "off",
    "no-underscore-dangle": ["error", { "allow": ["_execute"] }]
  },
  "globals": {
    "window": false,
    "Meteor": false,
    "Roles": false,
    "SimpleSchema": false
  }
}
